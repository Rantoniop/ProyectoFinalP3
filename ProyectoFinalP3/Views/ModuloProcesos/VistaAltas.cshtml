@model ProyectoFinalP3.Models.Altas


@using ProyectoFinalP3.Models

@{
    ViewBag.Title = "AgregarAtla";
    
}

<html>
<head>
    <title>Agregar Alta</title>

    <meta charset="utf-8" />

    <style>
        .panel {
            margin-top: 35px;
        }
    </style>
</head>
<body>
    <div>
        <div></div>
        <div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title text-center">Módulo Procesos</h4>
                </div>
                <div class="panel-body">


                    @Html.ActionLink("Volver atras", "Index")
                    <h2 class="text-center title">Ingresos de Altas</h2>
                    <div class="form-group">

                        @Html.ActionLink("Crear Alta", "AgregarAlta", "ModuloProcesos", new { @class = "btn btn-primary" })
                    </div>
                    <div class="form-group">
                        <table class="table table-bordered table-striped">
                            
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Fecha)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Monto)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IngresosId)
                                </th>
                                <th></th>
                            </tr>

                            @foreach (Altas altas in ViewBag.Altas)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => altas.Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => altas.Fecha)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => altas.Monto)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => altas.IngresosId)
                                    </td>
                                    <td>
                                        
                                        @Html.ActionLink("Borrar", "BorrarAlta", new { id = altas.Id })
                                    </td>
                                </tr>
                            }

                        </table>
                    </div>

                </div>
            </div>
        </div>


        <div></div>
    </div>
</body>
</html>