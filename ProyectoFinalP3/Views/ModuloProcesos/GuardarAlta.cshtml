@model ProyectoFinalP3.Models.Altas


@{
    ViewBag.Title = "IngresoPaciente";


    List<SelectListItem> itemsVA = (List<SelectListItem>)ViewBag.itemsVA;

}

<html>
<head>
    <title>Agregar Cita</title>
    <style>
        .panel {
            margin-top: 35px;
            max-width: 350px;
            min-width: 300px;
        }
    </style>
</head>
<body>
    <div>
        <div></div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title text-center">Ingreso Alta</h4>
            </div>
            <div class="panel-body">
                <div>
                    <div class="form-group">
                        <h4>Datos de Ingreso</h4>
                    </div>

                    @using (Html.BeginForm("GuardarAlta", "ModuloProcesos", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        <div class="form-group">
                            @Html.Label("Fecha de Alta")
                            @Html.EditorFor(a => a.Fecha, new { @class = "form-control" })
                            @Html.ValidationMessageFor(a => a.Fecha, "", new { @class = "text-danger" })
                        </div>
                        
                        <div>
                            <input type="submit" value="Agregar" class="btn btn-primary" /> |
                            @Html.ActionLink("Volver atras", "InfoAlta")
                        </div>
                    }
                </div>
            </div>
        </div>
        <div></div>
    </div>
</body>
</html>